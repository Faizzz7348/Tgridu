{
  "name": "tgridu-file-manager",
  "version": "1.0.0",
  "description": "Premium File Manager with Light/Dark Mode",
  "private": true,
  "scripts": {
    "dev": "bash kill-ports.sh && concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && npm run dev",
    "start:backend": "cd backend && npm start",
    "build": "vite build",
    "preview": "vite preview",
    "install:all": "npm install && cd backend && npm install",
    "kill-ports": "bash kill-ports.sh"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^9.2.1",
    "vite": "^5.0.0"
  }
}
